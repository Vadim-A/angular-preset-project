{
  "name": "angular-preset-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "npx ng",
    "start": "ng serve",
    "build": "npm run ng -- build",
    "build:prod": "npm run ng -- build --prod",
    "test": "npx jest --passWithNoTests",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --passWithNoTests --collect-coverage",
    "e2e": "npm run ng e2e",
    "lint": "npx eslint {src,e2e}/**/*.ts",
    "lint:fix": "npx eslint {src,e2e}/**/*.ts --fix",
    "lint-staged": "npx lint-staged",
    "stylelint": "npx stylelint src/**/*.scss",
    "stylelint:fix": "npx stylelint src/**/*.scss --fix",
    "prettier": "prettier --write {src,e2e}/**/*.{js,ts,css,html}",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --tsconfig \"./tsconfig.app.json\"",
    "postinstall": "npm run ngcc && npx husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/forms": "11.0.3",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "normalize.css": "8.0.1",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.3",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "11.0.3",
    "@angular/compiler-cli": "11.0.3",
    "@types/jest": "26.0.20",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.6.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-formatting": "2.0.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "jest-preset-angular": "8.3.2",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "stylelint": "13.9.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-prettier": "1.1.2",
    "ts-node": "8.3.0",
    "typescript": "4.0.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,scss,html}": [
      "prettier --write"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  }
}
